onents/NewArtpost';\n\n\n\n/* \nto use ThemeContext in each component:\n1. import {ThemeContext}\n2. const {theme, toggleTheme} = useContext(ThemeContext)\n3. in example of the toggle theme (dark - light) -> className={\"class \" + theme}\n4. to toggle theme -> \n    <button className='theme-btn' onClick={toggleTheme}>\n        {theme === \"dark\" ? \"light theme\" : \"dark theme\"}\n      </button>\n*/\n\nfunction App() {\n  return (\n    <div className=\"App\">\n    <NavBar/>\n\n    <Routes>\n      {/* Authorization routes */}\n      <Route path=\"/signup\" element={<SignupPage/>}/>\n      <Route path=\"/login\" element={<LoginPage/>}/>\n\n      <Route path=\"/\" element={<LandingPage/>}/> {/* index page */}\n      <Route path=\"/home\" element={<HomePage/>}/> {/* index page */}\n      <Route path=\":userId\" /> {/* chat page */}\n      <Route path=\":userId/posts\" /> {/* chat page */}\n      <Route path=\":userId/favorites\" /> {/* chat page */}\n\n      <Route path=\"new-post/post\" element={<NewPost/>}/>\n      <Route path=\"new-post/artpost\" element={<NewArtpost/>}/>\n\n      {/* route for the error page with message */}\n      <Route path=\"*\" element={<ErrorPage/>}/>\n    </Routes>\n      \n    </div>\n  );\n}\n\nexport default App;\n","/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/context/Theme.context.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/context/Auth.context.js",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/SignupPage.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/ErrorPage.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/LandingPage.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/components/NewArtpost.jsx",["81","82","83"],[],"import React, { useState, useEffect } from 'react';\nimport {useNavigate } from 'react-router-dom';\nimport axios from \"axios\"\n\nconst API_URL = \"http://localhost:5005\"\n\n\nfunction NewArtpost() {\n\n    const [artist, SetArtist] = useState(\"\")\n    const [title, setTitle] = useState(\"\")\n    const [description, setDescription] = useState(\"\")\n    const [medium, setMedium] = useState(\"\")\n    const [year, setYear] = useState(\"\")\n    const [dimensions, setDimensions] = useState(\"\")\n    /* const [dimensionWidth, setDimensionWidth] = useState(\"\") */\n    const [art_image, setArt_image] = useState(\"\")\n    const [errorMessage, setErrorMessage] = useState(undefined);\n\n\n    const navigate = useNavigate()\n\n    const handleArtist = (e) => SetArtist(e.target.value)\n    const handleTitle = (e) => setTitle(e.target.value)\n    const handleDescription = (e) => setDescription(e.target.value)\n    const handleMedium = (e) => setMedium(e.target.value)\n    const handleYear = (e) => setYear(e.target.value)\n    const handleDimensions = (e) => setDimensions(e.target.value)\n    /* const handleDimensionWidth = (e) => setDimensionWidth(e.target.value) */\n    const handleArt_image = (e) => setArt_image(e.target.value)\n\n    const handleArtpostSubmit = (e) => {\n        e.preventDefault()\n        const requestBody = {artist, title, description, medium, year, dimensions, art_image}\n        axios.post(`${API_URL}/api/new-post/artpost`, requestBody)\n            .then(response => navigate(\"/home\"))\n            \n            .catch((error) => {\n                const errorDescription = error.response.data.errorMessage\n                setErrorMessage(\"this is an error you have\", errorDescription)\n            })\n    }\n\n    const uploadImage = (file) => {\n        return axios.post(\"http://localhost:5005/api/new-post/upload\", file)\n          .then(res => res.data)\n          .catch(err => {\n            console.log(err)\n          });\n      };\n\n      const handleFileUpload = (e) => {\n        // console.log(\"The file to be uploaded is: \", e.target.files[0]);\n        const uploadData = new FormData();\n        // imageUrl => this name has to be the same as in the model since we pass\n        // req.body to .create() method when creating a new movie in '/api/movies' POST route\n        uploadData.append(\"imageUrl\", e.target.files[0]);\n\n          uploadImage(uploadData)\n          .then((response) => {\n            // console.log(\"response is: \", response);\n            // response carries \"fileUrl\" which we can use to update the state\n            setArt_image(response.fileUrl);\n            console.log(response.fileUrl)\n          })\n          .catch(err => console.log(\"Error while uploading the file: \", err));\n      };\n\n  return (\n    <div>\n    <h1>Art Post submit form</h1>\n\n    <form onSubmit={handleArtpostSubmit}>\n\n        <label>\n        Artist:\n            <input type=\"text\" name=\"artist\" value={artist} onChange={handleArtist}/>\n        </label>\n\n        <label>\n        Title:\n            <input type=\"text\" name=\"title\" value={title} onChange={handleTitle}/>\n        </label>        \n\n        <label>\n        Description:\n            <input type=\"text\" name=\"description\" value={description} onChange={handleDescription}/>\n        </label>\n\n        <label>\n        Medium\n            <input type=\"text\" name=\"medium\" value={medium} onChange={handleMedium}/>\n        </label>\n\n        <label>\n        Year\n            <input type=\"number\" name=\"year\" value={year} onChange={handleYear}/>\n        </label>\n\n        <label>\n        Dimension\n            <input type=\"number\" name=\"dimensions\" value={dimensions} onChange={handleDimensions}/>\n        \n            {/* <input type=\"number\" placeholder='Width' name=\"dimensionWidth\" value={dimensionWidth} onChange={handleDimensionWidth}/> */}\n        </label>\n\n        <label>\n        Image\n            {/* file upload with no cloudinary */}\n            {/* <input type=\"file\" name='art_image' value={art_image} onChange={handleArt_image}/> */}\n            {/* file upload with cloudinary */}\n            <input type=\"file\" name='art_image'  onChange={ (e) => handleFileUpload(e)}/>\n        </label>\n\n    <button type=\"submit\"> Submit</button>\n    </form>\n\n    \n    </div>\n  )\n}\n\nexport default NewArtpost","/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/components/NavBar.jsx",["84","85"],[],"import navbarCSS from \"./navbar.module.css\"\nimport React, { useState, useEffect } from 'react';\n\nimport { Link} from 'react-router-dom';\n\n\nimport reactLogo from \"../images/logo192.png\"\n\nfunction NavBar() {\n\n  return (\n    <div className={navbarCSS.navbar}>\n      <img src={reactLogo} alt=\"\" />\n      \n      <h3 className={navbarCSS.h3}>Macartsy</h3>\n\n      <div className={navbarCSS.buttons}>\n        <Link to={\"/about\"}><button>About Page</button></Link>\n        <Link to={\"/home\"}><button>Home</button></Link>\n        <Link to={\"/signup\"}><button>Sign Up</button></Link>\n        <Link to={\"/login\"}><button>Login</button></Link>\n        <Link to={\"/new-post\"}><button>Create new Post</button></Link>\n        {/* <select>\n          <option><Link to={\"/UserId\"}><button>User Profile</button></Link></option>\n          <option><button>change the theme</button></option>\n          <option><Link to={\"/UserId\"}><button>Log Out</button></Link></option>\n        </select> */}\n      </div>\n    </div>\n  )\n}\n\nexport default NavBar","/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/LoginPage.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/HomePage.jsx",[],[],"/Users/cameronaitcheson-labarr/Desktop/Ironhack/Module3/Project/my-artsy-app-client/src/pages/NewPostPage.jsx",["86","87","88"]